<Application
    x:Class="HttpHeadersViewer.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Strings.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Images.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme/Colors.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme/Brushes.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme/Geometry.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
